alter table FAT_MOVIMENTO_CAPA
NOCHECK CONSTRAINT FATMOVCAPA_FATMOVCAPREF_FK;

 DELETE FROM FAT_MOVIMENTO_CAPA WHERE EMPRESA = 1 AND REVENDA = 1 AND 
 NUMERO_NOTA_FISCAL = 1330568 AND SERIE_NOTA_FISCAL = '11' AND TIPO_TRANSACAO = 'P01' AND CONTADOR = 0

alter table FAT_MOVIMENTO_CAPA
CHECK CONSTRAINT FATMOVCAPA_FATMOVCAPREF_FK;

SELECT 
  name,
  is_disabled,
  is_not_trusted
FROM sys.foreign_keys
where name= 'FATMOVCAPA_FATMOVCAPREF_FK';
